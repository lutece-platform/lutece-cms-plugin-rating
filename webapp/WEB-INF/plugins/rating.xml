<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
 
    <!-- Plugin Informations --> 
    <name>rating</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.3</version>
    <description>rating.plugin.description</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>rating.plugin.description</description>
    <provider>rating.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/document/document.png</icon-url>
    <copyright>Copyright 2001-2010 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>2.4.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    
    <!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->
    <db-pool-required>1</db-pool-required>
    
    
    <!--  Specific plugin CSS stylesheet   --> 
    <css-stylesheets>
        <css-stylesheet>../../js/jquery/plugins/star-rating/jquery.rating.css</css-stylesheet>
    </css-stylesheets>
    <!--  Specific plugin JAVASCRIPT   --> 
    <javascript-files>
        <javascript-file>rating/rating.js</javascript-file>
        <javascript-file>../jquery/plugins/star-rating/jquery.rating.js</javascript-file>
    </javascript-files>

    <!-- filters -->
    <filters>
        <filter>
            <filter-name>ratingDownloadCountFilter</filter-name>
            <url-pattern>/rating/download/*</url-pattern>
            <filter-class>fr.paris.lutece.plugins.rating.web.DownloadCountFilter</filter-class>
        </filter>
    </filters>
    
</plug-in>